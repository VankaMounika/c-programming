#include <stdio.h>

int main() {
    int m, n;

    printf("Enter 2 positive numbers\n");
    scanf("%d%d", &m, &n);

    printf(" numbers between %d and %d are:\n", m, n);
    while(m <= n)
    {
        printf("%d  ", m);
        m++;
    }

    printf("\n");

    return 0;
}

Output:

